{
  "name": "@manifoldco/swagger-to-ts",
  "description": "Generate TypeScript types from Swagger OpenAPI specs",
  "main": "dist/cjs",
  "version": "1.6.0",
  "engines": {
    "node": ">= 8.0.0"
  },
  "bin": {
    "swagger-to-ts": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manifoldco/swagger-to-ts.git"
  },
  "keywords": [
    "swagger",
    "typescript",
    "ts",
    "openapi",
    "node"
  ],
  "author": "drew@pow.rs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manifoldco/swagger-to-ts/issues"
  },
  "homepage": "https://github.com/manifoldco/swagger-to-ts#readme",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-copy-assets",
        {
          "files": [
            "bin/"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "build": "pika build",
    "pregenerate": "npm run build",
    "generate": "node pkg/bin/cli example/input.yaml -o example/output.d.ts && node pkg/bin/cli example/input.yaml -o example/nowrapper.ts --nowrapper && node pkg/bin/cli example/input.yaml -o example/filewarning.d.ts --injectWarning",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts src",
    "publish": "npm run build && pika publish",
    "pretest": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "version": "npm run build"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "fs-extra": "^8.1.0",
    "js-yaml": "^3.13.1",
    "meow": "^6.0.0",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.8.1",
    "@pika/plugin-copy-assets": "^0.8.1",
    "@pika/plugin-ts-standard-pkg": "^0.8.1",
    "@types/jest": "^25.1.1",
    "@types/js-yaml": "^3.12.2",
    "@types/prettier": "^1.19.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "ts-jest": "^25.1.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  }
}
